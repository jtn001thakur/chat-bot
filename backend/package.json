{
  "name": "backend",
  "version": "1.0.0",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "kill-node": "powershell -ExecutionPolicy Bypass -File kill-node.ps1",
    "kill-port": "powershell -Command \"Stop-Process -Id (Get-NetTCPConnection -LocalPort 3000).OwningProcess -Force\"",
    "pre-dev": "npm run kill-node && npm run kill-port",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "npm run lint && nodemon --trace-warnings src/server.js",
    "start": "npm run lint && node src/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.10.0",
    "morgan": "^1.10.0",
    "ua-parser-js": "^2.0.2",
    "uuid": "^11.0.5",
    "validator": "^13.12.0",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "eslint": "^9.20.1",
    "eslint-plugin-react": "^7.37.4",
